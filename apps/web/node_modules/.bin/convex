#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/repos/construct/node_modules/.pnpm/convex@1.23.0_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules/convex/bin/node_modules:/home/ubuntu/repos/construct/node_modules/.pnpm/convex@1.23.0_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules/convex/node_modules:/home/ubuntu/repos/construct/node_modules/.pnpm/convex@1.23.0_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules:/home/ubuntu/repos/construct/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/repos/construct/node_modules/.pnpm/convex@1.23.0_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules/convex/bin/node_modules:/home/ubuntu/repos/construct/node_modules/.pnpm/convex@1.23.0_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules/convex/node_modules:/home/ubuntu/repos/construct/node_modules/.pnpm/convex@1.23.0_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules:/home/ubuntu/repos/construct/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../convex/bin/main.js" "$@"
else
  exec node  "$basedir/../convex/bin/main.js" "$@"
fi
